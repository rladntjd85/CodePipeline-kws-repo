  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.erp_mes.erp.attendance.mapper.CommuteScheduleMapper">

	<!-- 출퇴근 기준시간 -->
	<select id="getCurrentSchedule" resultType="com.erp_mes.erp.attendance.dto.CommuteScheduleDTO">
		SELECT 
		    schedule_id,
		    work_start_time,
		    work_end_time,
		    apply_start_date
		FROM commute_schedule
		WHERE ROWNUM = 1

